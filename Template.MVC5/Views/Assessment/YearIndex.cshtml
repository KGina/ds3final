@model IEnumerable<AbantwanaWebMaster.Model.Year>

@{
    ViewBag.Title = "Year";
}

<h2>List Of Years</h2>

<p>
    @Html.ActionLink("Add Year", "Create", "Year")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.year)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.year)
            </td>

        </tr>
    }

</table>
